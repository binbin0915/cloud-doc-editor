{
  "name": "cloud-doc-editor",
  "sideEffects": false,
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "build-win": "electron-builder --win --ia32",
    "build-mac": "electron-builder -mwl",
    "compile": "npm run compile-main && npm run compile-render",
    "compile-main": "babel -D ./main.js",
    "compile-render": "webpack --config ./webpack.config.build.js --color --progress",
    "pack-mac": "npm run compile && npm run build-mac",
    "pack-win": "npm run compile && npm run build-win",
    "build": "npm run pack-win",
    "onlyEle": "nodemon --watch main.js --watch menuTemplate.js --exec \"electron main.js\"",
    "dev": "concurrently \"wait-on http://localhost:3000/App.html && electron ./main.js\" \"cross-env NODE_ENV=development BROWSER=none npm start\"",
    "start": "./node_modules/.bin/webpack-dev-server --config webpack.config.dev.js",
    "render-build": "./node_modules/.bin/webpack --config webpack.config.build.js --progress",
    "release": "electron-builder",
    "pack": "electron-builder --dir"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-import": "^1.0.0",
    "babel-plugin-react-hot-loader": "^3.0.0-beta.6",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "electron": "^6.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "react-hot-loader": "^4.0.0",
    "react-loadable": "^5.4.0",
    "terser-webpack-plugin": "^1.4.1",
    "wait-on": "^3.3.0",
    "webpack": "^4.10.2",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.5",
    "write-file-webpack-plugin": "^4.2.0"
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.5",
    "ali-oss": "^6.5.1",
    "antd": "^4.1.1",
    "axios": "^0.19.2",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "css-loader": "^0.28.11",
    "dotenv": "^8.2.0",
    "electron-is-dev": "^1.1.0",
    "electron-store": "^4.0.0",
    "events": "^3.1.0",
    "file-loader": "^1.1.11",
    "for-editor": "^0.3.5",
    "iconv-lite": "^0.5.1",
    "immutable": "^4.0.0-rc.12",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
    "uuid": "^3.3.2"
  },
  "build": {
    "appId": "ainuo5213",
    "productName": "cloud-doc-editor",
    "asar": true,
    "asarUnpack": [],
    "compression": "normal",
    "electronVersion": "6.0.0",
    "directories": {
      "buildResources": "dist"
    },
    "win": {
      "target": [
        "msi",
        "nsis"
      ],
      "icon": "icon/icon.png",
      "artifactName": "${productName}-Web-Setup-${version}.${ext}",
      "publisherName": "ainuo5213"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "language": "zh-Hans"
    },
    "extends": null,
    "files": [
      "*.js",
      "build",
      "dist",
      "node_modules"
    ],
    "extraResources": [
      {
        "from": "icon/",
        "to": "icon/"
      }
    ]
  }
}
